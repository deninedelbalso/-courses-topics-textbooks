package courses;

import javax.persistence.GeneratedValue;

public class Course {
	
	@javax.persistence.Id
	@GeneratedValue
	private long Id;
	
	private String name;
	private String description;
	
	public Course() {
	}
	
	
	public Course(String name, String description) {
		this.name = name;
		this.description = description;
	}


	public long getId() {
		return Id;
	}


	public String getName() {
		return name;
	}

}
