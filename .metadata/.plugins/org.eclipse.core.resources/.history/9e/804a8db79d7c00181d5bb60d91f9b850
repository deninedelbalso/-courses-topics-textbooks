package courses;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;

@Entity
public class Topic {

	@Id
	@GeneratedValue
	private long id;
	
	
	private String name;
	
	//default no args constructor required by jpa
	public Topic() {
	}
	

	public Topic(String name) {
		this.name = name;
	}

	public Object getName() {
		return name
	}

	public long getId() {
		return id;
	}

}
